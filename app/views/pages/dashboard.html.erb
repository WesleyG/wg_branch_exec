<!-- WGG adding Guages -->
<div class="box">
  <div class="box-header"><span class="title">Marketplace</span></div>
    <div class="box-content padded" style="text-align: center">
        <div class="justgage" 
        data-title="New Subscribers" 
        data-gauge-width-scale="0.3" 
        data-labels="true" 
        data-gauge-Color="#edebeb"
        data-val="1443"
        data-max="2000"
        id="1"
        ></div>    

        <div class="justgage" 
        data-title="Continuing Subscribers" 
        data-gauge-width-scale="0.3" 
        data-labels="true" 
        data-gauge-Color="#edebeb"
        id="2"
        data-val="5282"
        data-max="6000"
        ></div>  
  </div>
</div>
<!-- WGG end of adding Guages -->

<!-- WGG adding charts -->
<div class="row">
  <div class="col-md-12"> 
    <div class="box">
      <div class="box-header"><span class="title">Marketplace Total Enrollments by Month</span></div>
      <div class="box-content padded">
        <div class="xcharts-line-dotted" style="width: 100%; height: 300px"></div>
      </div>
    </div>
  </div>
</div>
<!-- WGG end of charts -->


<!-- WGG start of table -->
<div class="row">
  <div class="col-md-12">
    <div class="box-content-padded">
      <div class="box">
        <div class="box-header"><span class="title">Churn Report</span></div>
        <div class="box-content">
          <div class="box-content-padded">
            <%= render partial: "partials/data_tables_custom", locals: { custom_class: "dTable" } %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- WGG charts -->
<div class="row">
  <div class="col-md-12">
  <div class="box">
    <%= render 'partials/enrollment_chart' %>
  </div>
</div>
</div>  
